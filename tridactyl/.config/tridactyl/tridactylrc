set theme dark

set configversion 2.0

blacklistadd https://mail.google.com
blacklistadd https://outlook.office.com
blacklistadd https://feedly.com
blacklistadd https://unit.atlassian.net

unbind <c-e>

bind --mode=browser <c-e> tab #

bind /     fillcmdline find
bind ?     fillcmdline find -?

bind n findnext 1
bind N findnext -1

bind ,r source

bind yt tabduplicate
bind <c-y> tabdetach

" vim: set filetype=tridactyl
