[DEFAULT]
jobs = 4

[scripts]
checkout = git clone 'git@github.com:lervag/scripts.git' 'scripts'

[.dotfiles]
checkout = git clone 'git@github.com:lervag/dotfiles.git' '.dotfiles'

[.config/nvim]
checkout = git clone 'git@github.com:lervag/dotnvim.git' '.config/nvim'
skip = lazy

[.local/plugged/vimtex]
checkout = git clone 'git@github.com:lervag/vimtex' 'vimtex'
skip = lazy

[.local/plugged/wiki.vim]
checkout = git clone 'git@github.com:lervag/wiki.vim' 'wiki.vim'
skip = lazy

[.local/plugged/wiki-ft.vim]
checkout = git clone 'git@github.com:lervag/wiki-ft.vim' 'wiki-ft.vim'
skip = lazy

[.local/plugged/vim-sintef]
checkout = git clone 'git@github.com:lervag/vim-sintef' 'sintef'
skip = lazy

[.password-store]
checkout = git clone 'git@github.com:lervag/pass' '.password-store'
skip = lazy
